CREATE TABLE IF NOT EXISTS `{{table}}`(
    `id` BIGINT(20) UNSIGNED NOT NULL AUTO_INCREMENT,{{#each fields}}
    `{{column}}` {{columnType}}  NULL COMMENT '{{comment}}', {{/each}}
